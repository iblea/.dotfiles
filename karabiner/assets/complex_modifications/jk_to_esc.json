{
   "title": "jk to esc",
   "rules": [
        {
            "description": "jk to esc 1",
            "manipulators": [
                {
                    "type": "basic",
                    "from": {
                        "simultaneous": [
                            { "key_code": "j" },
                            { "key_code": "k" }
                        ]
                    },
                    "to": [{ "key_code": "escape" }]
                }
           ]
	    },
        {
            "description": "kl to change en/ko",
            "manipulators": [
                {
                    "type": "basic",
                    "from": {
                        "simultaneous": [
                            { "key_code": "k" },
                            { "key_code": "l" }
                        ]
                    },
                    "to": [{ "key_code": "f19" }]
                }
           ]
	    }
	]
}
