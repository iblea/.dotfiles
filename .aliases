    echo -e "\tfi" >> ./kill_vsc_tmp_script.sh
    echo -e "\tkill -9 \$ps_vscode" >> ./kill_vsc_tmp_script.sh
    echo -e "done" >> ./kill_vsc_tmp_script.sh
