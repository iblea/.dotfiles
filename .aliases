function gitsvnrev() {
    if [[ "$@" = "" ]]; then
        return
    fi

    local revinf="$@"
    if [[ "${revinf:0:1}" = "r" ]]; then
        for branch in $(git for-each-ref --format='%(refname)' refs/heads/); do
            local inform=$(git svn find-rev $@ $branch)
            if [[ "$inform" = "" ]]; then
                continue
            fi
            echo "branch (${branch}) : ${inform}"
        done
    else
        git svn find-rev $@
    fi
}

