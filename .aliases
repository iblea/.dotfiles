function gconflictundo()
{
    con_path=$@
    if [[ $con_path = "" ]]; then
        con_path=.
    fi
    # git status -s $con_path | grep "^AA " | awk -F '\''AA '\'' '\'' { print $2 } '\'' | xargs -I {} echo {}
    git status -s $con_path | grep "^AA " | awk -F 'AA ' ' { print $2 } ' | xargs -I {} git checkout --merge {}
}

