# absolute path grep
function agrepn() {
    local curpwd=$(pwd);
    local search_str=("$@")
    if [[ "$curpwd" != "/" ]]; then
        curpwd="${curpwd}/"
    fi
    if [[ "$1" = "exai" ]]; then
        search_str=("--exclude-dir=ailib" "${@:2}")
