function ll()
{
    lsoutput=$( ls -lhF --color=always $@ )
    size_col_width=15

    echo "$lsoutput" | head -n 1
    echo "$lsoutput" | tail -n +2 | awk '{
        printf "%-7s ", $5
        for(i=9;i<=NF;i++) printf "%s ", $i;
        print ""
    }'
}

function lla()
{
    lsoutput=$( ls -alhF --color=always $@ )
    size_col_width=15

    echo "$lsoutput" | head -n 1
    echo "$lsoutput" | tail -n +2 | awk '{
        printf "%-7s ", $5
        for(i=9;i<=NF;i++) printf "%s ", $i;
        print ""
    }'
}

