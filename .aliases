# Î™®Îì† Î≥ÄÍ≤ΩÏÇ¨Ìï≠ pull
function pullall() {
    git branch -r | grep -v "\->" | while read remote; do git branch --track "${remote#origin/}" "$remote"; done
    git branch -r | grep -v "\->" | while read remote; do git branch --set-upstream-to="$remote" "${remote#origin/}"; done
    git fetch --all
    git pull --all
}
