alias dateunix='date "+%s"'
function unixtimeint() {
    if [ $# -ne 1 ]; then
        echo "wrong argument"
        return 1;
    fi
    date -d @$@ "+%Y-%m-%d %H:%M:%S"
}
function unixtimedate() {
    if [ $# -eq 1 ]; then
        date -d "$@" +%s
    elif [ $# -eq 6 ]; then
        read fyear_ fmonth_ fday_ fhour_ fmin_ fsec_ <<< "$@"
        # 2006-01-02 15:04:05
        date -d "${fyear_}-${fmonth_}-${fday_} ${fhour_}:${fmin_}:${fsec_}" +%s
        unset fyear_
        unset fmonth_
        unset fday_
        unset fhour_
        unset fmin_
        unset fsec_
    else
        echo "wrong argument."
        echo "write format \"2006-01-02 15:04:05\" or \"2006 01 02 15 04 05\""
        return 1
    fi
    return 0
}


