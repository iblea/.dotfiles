        return 1
    fi

    if [ ! -d "${gitroot}/.git" ]; then
        echo "not git directory in repo"
        return 1
    fi

    if [ ! -d "${gitroot}/.git/svn" ]; then
        echo "not svn directory in repo"
        return 1
    fi

    git svn log $@ 2>/dev/null
}


